<%- include('./partials/header') %>

    <p id="currentUser">
        <%= currentUser %>
    </p>

    <div>
        <% allUsers.forEach(function(userId) { %>
            <li>
                <a href="/chat/<%= userId.id %>"><%= userId.email %></a>
            </li>
        <% }); %>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.js"></script>

<%- include('./partials/footer') %>